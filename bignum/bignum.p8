---- calculating bignums
print('some big numbers\n----------------')
for i=15,27 do
 n = bignum(2)^i
 print('2^'..i..':'..show(n))
end
print('...')
print('2^64:'..show(bignum(2)^64))
n = factorial(52)
s = show(n)
print('52!:\0')
for i=0,#s/28 do
 if i == 0 then
  print(sub(s, 28*i+1, 28*i+29))
 else
  print('    '..sub(s, 28*i+1, 28*i+29))
 end
end

